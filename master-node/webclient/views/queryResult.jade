extends layout

block content
	h1 Query
	
	h2 Text
	pre= queryText

	h2 Reduce Mode
	p union

	h1 Results

	table.data
		thead
			tr
				each v, k in data[0]
					th= k
		tbody
			each row in data
				tr
					each field in row
						td= JSON.stringify(field)

	h2 Raw (JSON)

	pre.data= JSON.stringify(data)

